{
  "name": "demoelectronangularjses6",
  "version": "1.0.0",
  "description": "Example starter app for Electron, AngularJS 1.x, ES6, SASS",
  "main": "src/main.js",
  "license": "MIT",
  "scripts": {
    "start-dev":          "electron ./build-publish-develop",
    "start-prod":         "electron ./build-publish-production",
    "build-dev-linux":    "electron-packager ./build-publish-develop DemoAngular                     --version=0.36.9 --asar=true --platform=linux  --arch=x64  --overwrite --out ./build",
    "build-dev-windows":  "electron-packager ./build-publish-develop DemoAngular --app-version=1.0.0 --version=0.36.9 --asar=true --platform=win32  --arch=ia32 --overwrite --out ./build",
    "build-dev-mac":      "electron-packager ./build-publish-develop DemoAngular --app-version=1.0.0 --version=0.36.9 --asar=true --platform=darwin --arch=x64  --overwrite --out ./build",
    "build-prod-linux":   "electron-packager ./build-publish-production DemoAngular                     --version=0.36.9 --asar=true --platform=linux  --arch=x64  --overwrite --out ./build",
    "build-prod-windows": "electron-packager ./build-publish-production DemoAngular --app-version=1.0.0 --version=0.36.9 --asar=true --platform=win32  --arch=ia32 --overwrite --out ./build",
    "build-prod-mac":     "electron-packager ./build-publish-production DemoAngular --app-version=1.0.0 --version=0.36.9 --asar=true --platform=darwin --arch=x64  --overwrite --out ./build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Oceanware/demoelectronangularjses6.git"
  },
  "author": {
    "name": "Karl Shifflett",
    "url": "http://karlshifflett.wordpress.com"
  },
  "bugs": {
    "url": "https://github.com/oceanware/demoelectronangularjses6"
  },
  "homepage": "https://github.com/oceanware/demoelectronangularjses6",
  "keywords": [
    "electron",
    "ES6",
    "ES2015",
    "angularjs",
    "sass",
    "scss"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "devDependencies": {
    "del": "^2.2.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.9",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.0",
    "gulp-compass": "^2.1.0",
    "gulp-eslint": "^2.0.0",
    "gulp-jspm": "^0.5.7",
    "gulp-shell": "^0.5.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-watch": "^4.3.5",
    "jspm": "^0.16.30",
    "run-sequence": "^1.1.5"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "angular": "npm:angular@1.4.8",
      "angular-ui-router": "npm:angular-ui-router@^0.2.18"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
